<% var x=new Date(date); var today=new Date();%>
<% if(today.toDateString()==x.toDateString()){%>
    <h4 class="text-primary"><%=x.toDateString()%></h4>
    <%}else{%>
<h4><%=x.toDateString()%></h4>
<%}%>
<div class="table-responsive">
<table class="table table-striped table-bordered table-light table-hover">
    <thead class="thead-dark">
        <tr>
            <th scope="col" width="20%">Time</th>
            <th scope="col">Events</th>
        </tr>
    </thead>
    <tbody>
        <% for(var i=1;i<=12;i++){%>
            <tr>
                <th scope="row"><%=i%> AM</th> 
                <td>Mark</td>               
            </tr>
        <%}%>
        <% for(var i=1;i<=11;i++){%>
            <tr>
                <th scope="row"><%=i%> PM</th> 
                <td>Mark</td>               
            </tr>
        <%}%>
    </tbody>
</table>
</div>